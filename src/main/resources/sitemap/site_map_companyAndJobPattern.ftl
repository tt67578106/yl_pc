<?xml version="1.0" encoding="UTF-8"?>
<urlset>
	<!-- 表示news.163.com下的一组pattern对应关系-->
	<url>
		<loc><![CDATA[http://www.youlanw.com/]]></loc>
		<data>
			<display>
				<pc_url_pattern><![CDATA[http://www.youlanw.com/qiye_(\d+).html]]></pc_url_pattern>
				<xhtml_url_pattern ><![CDATA[http://m.youlanw.com/qiye_${1}.html]]></xhtml_url_pattern>
			</display>
		</data>
	</url>
	<url>
		<loc><![CDATA[http://www.youlanw.com/]]></loc>
		<data>
			<display>
				<pc_url_pattern><![CDATA[http://www.youlanw.com/zhaopin_(\d+).html]]></pc_url_pattern>
				<xhtml_url_pattern ><![CDATA[http://m.youlanw.com/zhaopin_${1}.html]]></xhtml_url_pattern>
			</display>
		</data>
	</url>
    <url>
        <loc><![CDATA[http://www.youlanw.com/]]></loc>
         <data>
             <display>
                 <pc_url_pattern><![CDATA[http://www.youlanw.com/]]></pc_url_pattern>
                 <xhtml_url_pattern ><![CDATA[http://m.youlanw.com/]]></xhtml_url_pattern>
             </display>
         </data>
    </url>
    <url>
        <loc><![CDATA[http://www.youlanw.com/]]></loc>
         <data>
             <display>
                 <pc_url_pattern><![CDATA[http://([^www]+).youlanw.com/]]></pc_url_pattern>
                 <xhtml_url_pattern ><![CDATA[http://m.youlanw.com/${1}/]]></xhtml_url_pattern>
             </display>
         </data>
    </url>
	<url>
		<loc><![CDATA[http://www.youlanw.com/]]></loc>
		<data>
			<display>
				<pc_url_pattern><![CDATA[http://youlanw.com/login]]></pc_url_pattern>
				<xhtml_url_pattern ><![CDATA[http://m.youlanw.com/login.html]]></xhtml_url_pattern>
			</display>
		</data>
	</url>
</urlset>